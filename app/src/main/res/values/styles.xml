<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="buttonoutlined" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="cornerRadius">8dp</item>
        <item name="strokeColor">@android:color/white</item>
        <item name="strokeWidth">1dp</item>
    </style>

    <style name="buttoninline" parent="Widget.MaterialComponents.Button">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:textColor">@color/blue_100</item>
        <item name="cornerRadius">8dp</item>
    </style>
</resources>