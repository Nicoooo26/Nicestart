<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="buttonoutlined" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="cornerRadius">8dp</item>
        <item name="strokeColor">@android:color/white</item>
        <item name="strokeWidth">1dp</item>
    </style>

    <style name="buttoninline" parent="Widget.MaterialComponents.Button">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:textColor">@color/blue_100</item>
        <item name="cornerRadius">8dp</item>
    </style>

    <style name="mytoast">
        <item name="stTextColor">#FFFFFF</item>
        <item name="stColorBackground">#FF4081</item>
        <item name="stRadius">20dp</item>
        <item name="stTextBold">true</item>
        <item name="stTextSize">14sp</item>
        <item name="stIconStart">@drawable/keyicon</item>
    </style>
</resources>